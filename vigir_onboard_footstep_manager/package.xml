<?xml version="1.0"?>
<package>
  <name>vigir_onboard_footstep_manager</name>
  <version>0.1.0</version>
  <description>The Onboard footstep manager package</description>
  <maintainer email="conner@torcrobotics.com">David Conner</maintainer>
  <license>BSD</license>
  <author email="conner@torcrobotics.com">David Conner</author>

  <buildtool_depend>catkin</buildtool_depend>
  
  <build_depend>roscpp</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>rviz</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>visualization_msgs</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>flor_ocs_msgs</build_depend>
  <build_depend>nodelet</build_depend>
  <build_depend>vigir_footstep_planning_msgs</build_depend>
  <build_depend>vigir_global_footstep_planner</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>actionlib_msgs</build_depend>  
  <build_depend>vigir_foot_pose_transformer</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>pluginlib</run_depend>
  <run_depend>rviz</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>visualization_msgs</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>flor_ocs_msgs</run_depend>
  <run_depend>nodelet</run_depend>
  <run_depend>vigir_footstep_planning_msgs</run_depend>
  <run_depend>vigir_global_footstep_planner</run_depend>
  <run_depend>actionlib</run_depend>
  <run_depend>actionlib_msgs</run_depend>
  <run_depend>vigir_foot_pose_transformer</run_depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <nodelet plugin="${prefix}/vigir_onboard_footstep_manager.xml" />
  </export>
</package>
